#Oracle
jdbc.handler oracleLobHandler
hibernate.jdbc.batch_size 10
hibernate.dialect org.hibernate.dialect.Oracle10gDialect
hibernate.connection.driver_class oracle.jdbc.driver.OracleDriver

hibernate.connection.username cmcsqpt
hibernate.connection.password cmcsqpt
hibernate.connection.url jdbc:oracle:thin:@10.1.8.193:1521:orcl
hibernate.connection.maxActive=20
hibernate.connection.maxIdle=2
hibernate.connection.maxWait=10000
hibernate.connection.defaultAutoCommit=false
hibernate.connection.removeAbandoned=true
hibernate.connection.removeAbandonedTimeout=30
hibernate.connection.logAbandoned=true
devMode true
