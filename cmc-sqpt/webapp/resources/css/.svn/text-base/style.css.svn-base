 /*top*/
 .top_bg{ width:100%; height:60px; background:url(../images/top_bg.jpg) repeat-x; border-bottom:1px solid #b9d1f0;}
.top{background:url(../images/top_bg.png) no-repeat; height:61px; width:100%;}
.logo{ padding-left:20px;float:left; margin-top:11px; height:50px;}

#navigation img { border:none;}
#navigation, #navigation ul {margin:0;padding:0;list-style:none;}
#navigation {float:right;background:url(../images/nav_bg.png) no-repeat left bottom; height:45px; line-height:45px; margin-top:16px; padding-right:8px;}

#navigation li.topmenu {float:left;text-align:center;position:relative;z-index:2; background:url(../images/line.png) right no-repeat;}
#navigation li.topmenu>a:link, #navigation li.topmenu>a:visited {display:block;font-size:14px;font-weight:bold;text-decoration:none;color:#2161a0;padding:0 15px;height:45px;line-height:45px;}

#navigation li.topmenu.hov a:hover{color:#2161a0; background:url(../images/nav_hov.png) no-repeat;}
#navigation li.topmenu>a:hover {color:#2161a0; background:url(../images/nav_hov.png) no-repeat;}
#navigation li.topmenu:hover .submenu{ display:block;}

#navigation li.topmenu .submenu {position: absolute;top: 33px;left: -5px;width: 138px; display:none; } 


#navigation .menu_connertop{ background:url(../images/dropdown_bg.png) no-repeat; height:15px;} 
#navigation .menu_connerbottom{ background:url(../images/dropdown_bg.png) no-repeat left -40px; height:15px; margin-top:-2px; }
#navigation .submenu  li{ background:url(../images/dropdown_bg.png) no-repeat left -14px;  height:26px; line-height:26px; padding-left:30px;text-align:left; }
#navigation .submenu  li a{ color:#2161a0; }
#navigation .submenu  li a:hover{ color:#2161a0; font-weight:bold;}
	

