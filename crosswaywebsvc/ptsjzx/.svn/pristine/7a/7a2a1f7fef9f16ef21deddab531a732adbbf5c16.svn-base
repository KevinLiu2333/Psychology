@charset "utf-8";
/*全局*/
body {
	margin:0;
	padding:0;
	font-size:12px;
	color:#333;
	font-family:"宋体",Arial, Helvetica, sans-serif;
	}
a:link,a:visited {
	font-size:12px;
	color:#333;
	text-decoration:none;
	}
a:hover {
	color:#FF0000;
	text-decoration:underline;
	}
	
/*登录页*/
#full-screen-slider { width:100%; height:295px; float:left; position:relative;}
#slides { display:block; width:100%; height:295px; list-style:none; padding:0; margin:0; position:relative}
#slides li { display:block; width:100%; height:100%; list-style:none; padding:0; margin:0; position:absolute}
#slides li a { display:block; width:100%; height:100%; text-indent:-9999px}
#pagination { display:block; list-style:none; position:absolute; left:60%; top:260px; z-index:9900;  padding:5px 15px 5px 0; margin:0}
#pagination li { display:block; list-style:none; width:10px; height:10px; float:left; margin-left:15px; border-radius:5px; background:#FFF }
#pagination li a { display:block; width:100%; height:100%; padding:0; margin:0;  text-indent:-9999px;}
#pagination li.current { background:#0092CE}

#login_logo {
	width:440px;
	height:140px;
	margin:0 auto;
	position:relative;
	top:-230px;
	z-index:999;
	}
#login_box {
	background:url(../images/login_box.png) no-repeat;
	width:440px;
	height:330px;
	margin:0 auto;
	top:-210px;
	position:relative;
	z-index:888;
	}
#login_input_bg {
	background:url(../images/login_input.png) no-repeat;
	width:280px;
	height:81px;
	}
.login_input {
	background:none;
	border:none;
	height:26px;
	line-height:26px;
	font-size:14px;
	width:180px;
	}
.login_checkbox {
	margin-left:10px;
	vertical-align:middle;
	}
.login_button,.login_button_hover {
	width:280px;
	height:44px;
	border:none;
	cursor:pointer;
	}
.login_button {
	background:url(../images/login_button.png) no-repeat;
	}
.login_button_hover {
	background:url(../images/login_button_hover.png) no-repeat;
	}
.login_link a:link,.login_link a:visited {
	text-decoration:underline;
	color:#999999;
	}
.login_link a:hover {
	color:#333333;
	}
	
/*页面顶部*/
#header td {
	background:url(../images/header_bg.jpg) repeat-x;
	height:39px;
	}
#user { float:right; height:39px; line-height:39px; color:#FFF;}
#navigation img { border:none;}
#navigation,#navigation ul {
	margin:0;
	padding:0;
	list-style:none;
	}
#navigation {
	float:right;
	}
#navigation li {
	float:left;
	text-align:center;
	position:relative;
	z-index:2;
}
#navigation li a:link, #navigation li a:visited {
	display:block;
	font-size:14px;
	font-weight:bold;
	text-decoration:none;
	color:#FFF;
	padding:0 10px;
	height:39px;
	line-height:39px;
}
#navigation li a:hover {
	color:#FFF;
	background:url(../images/nav_hover.jpg) repeat-x;
}
#navigation li ul li a:link, #navigation li ul li a:visited {
	display:block;
	width:82px;
	height:25px;
	color:#FFF;
	font-size:12px;
	font-weight:normal;
	line-height:25px;
	background:#026a8d;
	border-top:1px solid #FFF;
}
#navigation li ul li a:hover {
	color:#fff;
	background:#119ed0;
}
#navigation li ul {
	display:none;
	position:absolute;
	top:35px;
	left:0px;
	width:82px;
}
#navigation li ul li ul {
	display:none;
	position:absolute;
	top:0px;
	left:0px;
	margin-top:0;
	width:82px;
}

/*按钮选中*/
.tag {
 float:left;
 list-style:none;
 margin:0px;
 padding:0px;
}
.tag li {
 float:left;
 width:70px;
 height:28px;
 cursor:pointer;
 text-align:center;
 margin-right:5px;
 background:url(../images/tag.jpg) no-repeat;
}
.tag li.hover {
 background:url(../images/tag_selected.jpg) no-repeat;
 color:#000;
 width:70px;
 height:28px;
}

/*卡片选中*/
.card {
 float:left;
 list-style:none;
 margin:0 0 10px 0;
 padding:0px;
}
.card li {
 float:left;
 width:74px;
 height:77px;
 cursor:pointer;
 text-align:center;
 margin-right:5px;
 }
.card li span {
 width:74px;
 height:77px;
 display:block;
	}
.card li .card_a {
 background:url(../images/card_a.jpg) no-repeat;
	}
.card li.hover .card_a{
 background:url(../images/card_a_selected.jpg) no-repeat;
}
.card li .card_b {
 background:url(../images/card_b.jpg) no-repeat;
	}
.card li.hover .card_b{
 background:url(../images/card_b_selected.jpg) no-repeat;
}
.card li .card_c {
 background:url(../images/card_c.jpg) no-repeat;
	}
.card li.hover .card_c{
 background:url(../images/card_c_selected.jpg) no-repeat;
}
.number {
	clear:both;
	margin-top:5px;
	background: url(../images/number_bg.jpg) no-repeat;
	display:block;
	width:237px;
	height:34px;
	line-height:34px;
	text-align:center;
	font-size:18px;
	font-weight:bold;
	}


/*人物信息*/
#info {
	width:275px;
	background:url(../images/info_bg.png) repeat-x top;
	}
.info_title {
	color:#999;
	padding-left:5px;
	text-align:right;
	}
.info_content {
	text-align:left;
	line-height:22px;
	}
.much {
	padding-top:4px;
	}
.info_content span {
	color:#e10602;
	}
	
/*标题背景*/
.all_title {
	background:url(../images/all_title_bg.jpg) repeat-x;
	font-size:14px;
	font-weight:bold;
	padding:0 20px;
	height:25px;
	color:#2f91b4;
	line-height:25px;
	}


/*菜单部分*/
.menu {
	width:147px;
	
	}
.menu ul {
	margin:0;
	padding:0;
	}
.menu ul li {
	border:1px solid #a5bbcd;
	margin-bottom:10px;
	display:block;
	width:147px;
	background: url(../images/menu_title.jpg) top left repeat-x;
	list-style:none;
	overflow:visible;
}
.menu ul li a {
	display:block;
	height:30px;
	width:132px;
	padding-left:15px;
	line-height:30px;
	font-size:14px;
	color:#FFF;
	font-weight:bold;
	outline:none;
	text-decoration:none;
}
.menu ul li .active {
	background:url(../images/on.png) top right no-repeat;
}

.menu ul li .inactive {
	background:url(../images/off.png) top right no-repeat;
}
.menu ul li ul {
	display:none;
}
.menu ul li ul li {
	border:none;
	margin-bottom:0;
	background:#FFF;
	display:block;
	font-size:12px;
	color:#8f9d4c;
	width:132px;
	padding-left:15px;
}
.menu ul li ul li a:link,.menu ul li ul li a:visited {
	background:none;
	font-size:12px;
	display:block;
	padding:0px 0;
	color:#333333;
	font-weight:normal;
	padding-left:0px; 
}
.menu ul li ul li a:hover {
	color:#F00;
	text-decoration:underline;
	}
	
/*人物信息背景*/
#info_menu {
	width:285px;
	height:400px;
	background:url(../images/info_menu_bg.jpg) no-repeat center bottom;
	}
	
/*查询菜单背景*/
#menu_bg {
	background:#e5eef3 url(../images/menu_bg.jpg) no-repeat left bottom;
	}
