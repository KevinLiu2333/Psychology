package com.wonders.wddc.suite.csrq.entity;

import org.nutz.dao.entity.annotation.Column;
import org.nutz.dao.entity.annotation.Name;
import org.nutz.dao.entity.annotation.Table;

@Table("suite_report_data")
public class ReportDataBo {
	@Name
	@Column("id")
	private String id;
	//报表id
	@Column("report_info_id")
	private String reportinfoid;
	//数据项id
	@Column("mtdm_id")
	private String mtdmid;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getReportinfoid() {
		return reportinfoid;
	}
	public void setReportinfoid(String reportinfoid) {
		this.reportinfoid = reportinfoid;
	}
	public String getMtdmid() {
		return mtdmid;
	}
	public void setMtdmid(String mtdmid) {
		this.mtdmid = mtdmid;
	}
}
