<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%>
<%@ include file="/cj/title_setting.jsp" %>
<div class="row">
    <div class="col-md-12 form-group">
        服务名称：<input type="text" name="fwInfo.fwName" style="width:785px"/>
    </div>
     <div class="col-md-6 form-group">
        公开类型：<select name="fwInfo.openType" style="width:200px;height: 30px">
            <option value="">----</option>
            <option value="直接公开">直接公开</option>
            <option value="申请公开">申请公开</option>
            <option value="不公开">不公开</option>
        </select>
    </div>
    <div class="col-md-6 form-group">
        申请类型：<select name="fwInfo.applyType" style="width:200px;height: 30px">
            <option value="">----</option>
            <option value="需要审批">需要审批</option>
            <option value="不需要审批">不需要审批</option>
        </select>
    </div>
    <div class="col-md-6 form-group">
        服务状态：<select name="fwInfo.status" style="width:200px;height: 30px">
            <option value="">----</option>
            <option value="正常">正常</option>
            <option value="停止">停止</option>
        </select>
    </div>
    <div class="col-md-6 form-group">
        预警级别：<select name="fwInfo.alertLevel"  style="width:200px;height: 30px">
            <option value="">----</option>
            <option value="一级">一级预警</option>
            <option value="二级">二级预警</option>
            <option value="三级">三级预警</option>
        </select>
    </div>
    <div class="col-md-12 form-group">
        服务标签：<select multiple="" id="tagList" name="fwInfo.tagList" class="select2" data-placeholder="请选择标签...">
            <option value="">&nbsp;</option>
        </select>
    </div>
    <div class="col-md-12 form-group">
        服务描述： <textarea name="fwInfo.fwAbstract"  class="form-control" rows="3" style="width:865px;"></textarea>
    </div>

</div>