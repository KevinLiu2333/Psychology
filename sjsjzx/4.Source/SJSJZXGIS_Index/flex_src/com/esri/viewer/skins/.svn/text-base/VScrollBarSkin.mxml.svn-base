<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark">

    <fx:Metadata>
        [HostComponent("spark.components.VScrollBar")]
    </fx:Metadata>

    <s:states>
        <s:State name="normal"/>
        <s:State name="disabled"/>
        <s:State name="inactive"/>
    </s:states>

    <s:Button id="track"
              top="12" bottom="12"
              focusEnabled="false"
              horizontalCenter="0"
              skinClass="com.esri.viewer.skins.VScrollBarTrackSkin"/>

    <s:Button id="thumb"
              focusEnabled="false"
              horizontalCenter="0"
              skinClass="com.esri.viewer.skins.VScrollBarThumbSkin"
              visible.inactive="false"/>

    <s:Button id="decrementButton"
              top="0"
              enabled.inactive="false"
              focusEnabled="false"
              skinClass="com.esri.viewer.skins.ScrollBarUpButtonSkin"/>

    <s:Button id="incrementButton"
              bottom="0"
              enabled.inactive="false"
              focusEnabled="false"
              skinClass="com.esri.viewer.skins.ScrollBarDownButtonSkin"/>

</s:SparkSkin>
