<?xml version="1.0" ?>
<configuration>
	<layers>
		<extentLayer>
			<name>工程范围</name>
			<url>arcgis/rest/services/dynamic_pdgt2012/MapServer/7</url>
			<expression>XZ_C5 like '%[value]%' </expression>
			<graphicalsearchlabel>从下面选择一种多边形以选定查询范围：</graphicalsearchlabel>
			<fields>GC_ID,GC_C10</fields>
			<fieldsCN>GC_ID</fieldsCN>
			<titlefield>GC_ID</titlefield>
			<linkfield>GC_ID</linkfield>
			<icon>i_uni1.png</icon>
		</extentLayer>
	</layers>
	<infodata>
		<url>/PDGT_Index/servlet/GetOracleData</url>
		<table>DC_CONSTRUCTIONS_PERMIT</table>
		<fields>GC_ID,PROJECT_NAME,PASS_NO,APPROVE_DATE,BUILD_UNIT,PROJECT_TYPE_NAME,BUILD_ADDRESS</fields>
		<fieldsCN>关联号,项目名称,证       号,批复日期,建设单位,工程性质,地       址</fieldsCN>
	</infodata>
	<linkURL>/pdgtdbc/common/gisRedirect.jsp?funId=gongchengxukezhengjianzhubiaozhun&gcId=</linkURL>
	<geometryserviceURL>arcgis/rest/services/Geometry/GeometryServer</geometryserviceURL>
	<excelservice>/EMGIS_Java/servlet/ExcelExport</excelservice>
	<years>
	     <url>widgets/Query/years.xml</url>
	</years>
</configuration>
