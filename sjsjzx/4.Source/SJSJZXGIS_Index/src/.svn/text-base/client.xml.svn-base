<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
<!-- 统一人员组织服务 ||正式环境：10.1.8.71:9888  ||测试环境192.68.55.3:9888 ||863环境：10.1.8.71:9888-->
<bean id="transAccountService" class="com.wonders.security.client.factory.XFireClientFactoryBean">
	<property name="serviceClass" 
	value="com.wonders.security.spi.AccountService"/>
    <property name="wsdlDocumentUrl" 
    value="http://10.1.8.71:9888/services/AccountService"/>
</bean>

<!-- 统一授权服务 -->
<bean id="transAuthorizationService" class="com.wonders.security.client.factory.XFireClientFactoryBean">
	<property name="serviceClass" 
	value="com.wonders.security.spi.AuthorizationService"/>       
    <property name="wsdlDocumentUrl" 
    value="http://10.1.8.71:9888/services/AuthorizationService"/>
</bean>

<!-- 统一认证服务 -->
<bean id="transAuthenticationService" class="com.wonders.security.client.factory.XFireClientFactoryBean">
	<property name="serviceClass" 
	value="com.wonders.security.spi.AuthenticationService"/>       
    <property name="wsdlDocumentUrl" 
    value="http://10.1.8.71:9888/services/AuthenticationService"/>
</bean>

<!-- 统一日志审计服务 -->
<bean id="transAuditService" class="com.wonders.security.client.factory.XFireClientFactoryBean">
	<property name="serviceClass" 
	value="com.wonders.security.spi.AuditService"/>       
    <property name="wsdlDocumentUrl" 
    value="http://10.1.8.71:9888/services/AuditService"/>
</bean>

<!-- 统一应用服务 -->
<bean id="transApplicationService" class="com.wonders.security.client.factory.XFireClientFactoryBean">
	<property name="serviceClass" 
	value="com.wonders.security.spi.ApplicationService"/>       
    <property name="wsdlDocumentUrl" 
    value="http://10.1.8.71:9888/services/ApplicationService"/>
</bean>
</beans>