package com.wonders.gis.utils;
/**
 * @author XiaQiuQin
 * 字段映射
 */
public class Field {
	private String fieldName = "";		//字段名称
	private int fieldType;				//字段类型
	private String fieldTypeName = "";	//类型名称
	private int scale = 0;				//长度
	private int precision = 0;			//精度
	
	/**
	 * 构造函数
	 */
	public Field(){
		
	}
	
	/** 构造函数
	 * @param fieldName 字段名称
	 */
	public Field(String fieldName){
		this.fieldName = fieldName;
	}
	
	/** 构造函数
	 * @param fieldName 字段名称
	 * @param fieldType 字段类型
	 * @param scale		长度
	 * @param precision 精度
	 */
	public Field(String fieldName, int fieldType, int scale, int precision){
		this.fieldName = fieldName;
		this.fieldType = fieldType;
		this.scale = scale;
		this.precision = precision;
	}

	/**
	 * @return the fieldName
	 */
	public String getFieldName() {
		return fieldName;
	}

	/**
	 * @param fieldName the fieldName to set
	 */
	public void setFieldName(String fieldName) {
		this.fieldName = fieldName;
	}

	/**
	 * @return the fieldType
	 */
	public int getFieldType() {
		return fieldType;
	}

	/**
	 * @param fieldType the fieldType to set
	 */
	public void setFieldType(int fieldType) {
		this.fieldType = fieldType;
	}

	/**
	 * @return the fieldTypeName
	 */
	public String getFieldTypeName() {
		return fieldTypeName;
	}

	/**
	 * @param fieldTypeName the fieldTypeName to set
	 */
	public void setFieldTypeName(String fieldTypeName) {
		this.fieldTypeName = fieldTypeName;
	}

	/**
	 * @return the scale
	 */
	public int getScale() {
		return scale;
	}

	/**
	 * @param scale the scale to set
	 */
	public void setScale(int scale) {
		this.scale = scale;
	}

	/**
	 * @return the precision
	 */
	public int getPrecision() {
		return precision;
	}

	/**
	 * @param precision the precision to set
	 */
	public void setPrecision(int precision) {
		this.precision = precision;
	}
}
