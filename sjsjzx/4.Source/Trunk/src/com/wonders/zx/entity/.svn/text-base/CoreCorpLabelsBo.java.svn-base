package com.wonders.zx.entity;

import java.util.Date;

import org.nutz.dao.entity.annotation.Column;
import org.nutz.dao.entity.annotation.Table;

/**
 * @author Wanda
 * 法人基本信息标签关联表实体类
 */
@Table("SJTZ_CORE_CORP_LABELS")
public class CoreCorpLabelsBo {
	/**
	*ID
	**/
	@Column("ID")
	private String id;
	/**
	*法人核心表ID
	**/
	@Column("CORE_CORP_ID")
	private String corecorpid;
	/**
	*法人名称
	**/
	@Column("CORP_NAME")
	private String corpname;
	/**
	*统一社会信用代码
	**/
	@Column("UNI_SC_ID")
	private String uniscid;
	/**
	*组织机构代码
	**/
	@Column("ORGAN_CODE")
	private String organcode;
	/**
	*标签集IDs
	**/
	@Column("LABEL_IDS")
	private String labelids;
	/**
	*标签集汉字
	**/
	@Column("LABEL_HZS")
	private String labelhzs;
	/**
	*标签更新日期
	**/
	@Column("LABEL_DATE")
	private Date labeldate;
}
