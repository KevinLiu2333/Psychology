@charset "utf-8";
/* CSS Document */
*{
	padding:0;
	margin:0;
	-moz-user-select: none; /*火狐*/
	-webkit-user-select: none; /*webkit浏览器*/
	-ms-user-select: none; /*IE10*/
	-khtml-user-select: none; /*早期浏览器*/
	user-select: none;
}
ul{list-style:none;}

/* 登录样式2 */
.loginBack2{background:#abe6fa;}
.login_all2{width:1200px; height:680px; position:absolute; left:50%; top:50%; margin:-365px 0 0 -600px; background:url(../images/loginBack_2.png) no-repeat center center;}
.login_all2 ul{list-style:none; padding:292px 0 0 465px; font-size:14px; font-family:"微软雅黑"; color:#545454;}
.login_all2 ul li{height:35px; line-height:35px; padding:10px 6px;}
.login_all2 ul li span{display:block; display:inline-block; width:52px;}
.login_all2 ul li input[type="text"], .login_all2 ul li input[type="password"]{width:220px; height:25px; line-height:25px; padding:5px 8px; border-radius:2px; background:#f4f4f4; border:#c3c3c3 1px solid; color:#818181; font-size:14px; font-family:"微软雅黑"; -moz-box-shadow:2px 2px 2px #ccc inset; -webkit-box-shadow:2px 2px 2px #ccc inset; box-shadow:1px 1px 2px #ccc inset; }
.login_all2 ul li input[type="button"]{width:116px; height:43px; line-height:43px; text-align:center; background:url(../images/enter_btn2.png) no-repeat left top; color:#fff; font-family:"微软雅黑"; font-size:14px; border:none; cursor:pointer;}
.login_all2 ul li input[type="button"]:hover{background:url(../images/enter_btn_hover2.png) no-repeat left top;}
.login_all2 ul li input[type="submit"]{width:116px; height:43px; line-height:43px; text-align:center; background:url(../images/enter_btn2.png) no-repeat left top; color:#fff; font-family:"微软雅黑"; font-size:14px; border:none; cursor:pointer;}
.login_all2 ul li input[type="submit"]:hover{background:url(../images/enter_btn_hover2.png) no-repeat left top;}

/* 首页样式 */
.indexBody{background:url(../images/topbg.jpg) no-repeat center 3px;}
.indexMain{width:1003px; margin:0 auto; font-family:"微软雅黑";}
.topLine{width:100%; height:35px; line-height:29px; background:url(../images/topLine.png) repeat-x top;}
.topLine ul{width:998px; margin:0 auto; font-size:12px; text-align:right;}
.topLine ul li{display:inline-block; background:url(../images/welcomeIcon.png) no-repeat left 6px; padding-left:21px; margin-right:16px; color:#474747;}
.topLine ul li.exit{background:url(../images/exit.png) no-repeat left 7px; padding-left:20px;}
.topLine ul li.exit a{color:#187786;}
.topLine ul li.exit a:hover{color:#333; text-decoration:none;}
.head{width:100%; height:138px; padding-top:10px;}
.head img{margin-left:-20px; margin-top:9px;}
.nav{width:100%; height:44px; border-bottom:#00857e 2px solid; overflow:visible; position:relative;}
.navScreen{width:1002px; height:44px; overflow:hidden; position:relative;}
.nav .navLeft, .nav .navRight{width:20px; height:30px; background:url(../images/navLeft.png) no-repeat right 3px top; position:absolute; overflow:hidden; text-indent:-200px; left:-24px; top:15px; cursor:pointer; display:none;}
.nav .navRight{background:url(../images/navRight.png) no-repeat 4px top; left:1003px;}
.nav ul{list-style:none; position:absolute; left:0;}
.nav ul li{float:left; margin-right:2px; width:165px; text-align:center; height:auto; overflow:visible; position:relative;}
.nav ul li dl{display:block; width:160px; position:absolute; z-index:999; background:#0068b7; border:#0068b7 5px solid; border-bottom-left-radius:4px; border-bottom-right-radius:4px; border-top:none; display:none;}
.nav ul li dl dt a, .nav ul li:hover dl dt a{display:block; background:#d9e8f4 url(../images/sub_icon.png) no-repeat 12px 11px; border-radius:2px; color:#5b5c5d; margin-top:5px; font-size:14px; text-align:left; padding-left:40px; width:120px; line-height:40px; height:40px;}
.nav ul li dl dt a:hover{background:#ffffff url(../images/sub_icon_hov.png) no-repeat 12px 11px; color:#0068b7; border-radius:2px; font-size:16px;}
.nav ul li a{display:block; width:100%; height:44px; line-height:44px; overflow:hidden; font-family:"微软雅黑";}
.nav ul li:hover a{background:#00857e; border-top-left-radius:4px; border-top-right-radius:4px; color:#ffffff;}
.nav ul li a{color:#00857e; text-decoration:none;}
.nav ul li a span{display:inline-block; width:24px; height:24px; vertical-align:middle; margin:-2px 7px 0 0;}
.nav ul li a span.icon1{background:url(../images/micon.png) no-repeat left top;}
.nav ul li a span.icon2{background:url(../images/micon.png) no-repeat -24px top;}
.nav ul li a span.icon3{background:url(../images/micon.png) no-repeat -48px top;}
.nav ul li a span.icon4{background:url(../images/micon.png) no-repeat -72px top;}
.nav ul li a span.icon5{background:url(../images/micon.png) no-repeat -96px top;}
.nav ul li a span.icon6{background:url(../images/micon.png) no-repeat -120px top;}
.nav ul li a span.icon7{background:url(../images/micon.png) no-repeat -240px top;}
.nav ul li:hover a span.icon1{background:url(../images/micon.png) no-repeat left -24px;}
.nav ul li:hover a span.icon2{background:url(../images/micon.png) no-repeat -24px -24px;}
.nav ul li:hover a span.icon3{background:url(../images/micon.png) no-repeat -48px -24px;}
.nav ul li:hover a span.icon4{background:url(../images/micon.png) no-repeat -72px -24px;}
.nav ul li:hover a span.icon5{background:url(../images/micon.png) no-repeat -96px -24px;}
.nav ul li:hover a span.icon6{background:url(../images/micon.png) no-repeat -120px -24px;}
.nav ul li:hover a span.icon7{background:url(../images/micon.png) no-repeat -240px -24px;}

.moreAll{width:790px; height:514px; background:#fff; border:#00857e 4px solid; border-radius:4px; position:absolute; right:-2px; top:0; z-index:9999; display:none;}
.moreContent{width:788px; height:514px; overflow-y:auto; padding-left:4px;}
.moreAll .leftArrow{display:inline-block; width:8px; height:15px; background:url(../images/leftArrow.png) no-repeat left top; position:absolute; left:-12px; top:4px;}
.moreAll .itemIn{width:148px; display:inline-block; margin:6px 0 0 4px;}
.moreAll .itemTitle{height:28px; line-height:28px; padding:0 8px; background:#e6e6e6; border-radius:4px; color:#575454; font-size:14px;}
.moreAll ul{list-style:none; margin:0 2px;}
.moreAll ul li{background:url(../images/xuxian.png) repeat-x bottom; padding:6px 0; height:22px; line-height:22px; color:#047973;}
.moreAll ul li span{display:inline-block; width:71px; height:22px; line-height:22px; text-align:center; color:#616161; padding-right:5px; margin-right:2px;}
.moreAll ul li:nth-child(2){background:none;}

.main{width:100%; margin:12px 0; font-size:12px; font-family:Verdana, Geneva, sans-serif; position:relative;}
.mainLeft{width:190px; float:left; position:relative; background:#00857e; border-radius:4px; padding:4px; padding-top:2px;  margin-left: 20px}
.leftTitle{background:url(../images/flag.png) no-repeat 2px 3px; height:28px; line-height:28px; color:#fff; font-family:"微软雅黑"; font-size:14px; padding-left:22px;}
.leftMore{width:56px; height:28px; line-height:28px; position:absolute; right:0; top:2px; text-align:center; color:#fff; font-size:12px; text-decoration:underline; cursor:pointer; font-family:"宋体";}
.leftMore:hover{text-decoration:none;color:#ff0;}
.leftContent{border-radius:2px; background:#fff; height:482px; padding:6px 6px 0 6px;}
.itemTitle{height:28px; line-height:28px; padding:0 8px; background:#e6e6e6; border-radius:4px; color:#575454; font-size:14px;}
.leftContent ul{list-style:none; margin:0 2px;}
.leftContent ul li{background:url(../images/xuxian.png) repeat-x bottom; padding:6px 0; height:22px; line-height:22px; color:#616161;}
.leftContent ul li span{display:inline-block; width:71px; height:22px; line-height:22px; text-align:center; color:#fff; background:url(../images/greenLeft.png) no-repeat left top; padding-right:5px; margin-right:6px;}
.leftContent ul li:nth-child(2){background:none;}
.leftContent ul li:nth-child(2) span{background:url(../images/blueLeft.png) no-repeat left top;}

.mainRight{width:790px; height:520px; float:left; margin-left: 20px}
.rightTitle{font-family:"微软雅黑"; font-size:21px; color:#00857e; background:url(../images/rightTitle.png) no-repeat 2px 4px; padding-left:38px; height:40px; line-height:40px;}
#monitorAll{width:100%; height:480px; position:relative;}
.centerData{width:100%;height:50px; line-height:50px; background:url(../images/dataCenter.png) no-repeat left top; text-align:center; font-family:"微软雅黑"; font-size:18px; color:#fff; position:absolute; left:0; top:50%; margin-top:-40px; z-index:999;}
#monitorAll ul{list-style:none; color:#454545;}
#monitorAll ul li{display:block; position:absolute; background:url(../images/line.png) no-repeat 61px top; width:100px;}
#monitorAll ul li a{display:block; width:60px; border-bottom:#f00 2px solid; position:absolute; left:2px; top:-24px; line-height:24px; height:24px; cursor:pointer; text-decoration:none; color:#454545;}
#monitorAll ul li a.good{border-bottom:#76ee01 2px solid;}
#monitorAll ul li a.bad{border-bottom:#f00 2px solid;}


#monitorAll ul li.below{background:url(../images/line.png) no-repeat right bottom; width:100px;}
#monitorAll ul li.below a{display:block; width:60px; text-align:center; border:none; border-top:#f00 2px solid; position:absolute; left:auto; right:-60px; top:auto; bottom:-24px; line-height:22px; height:22px; cursor:pointer; overflow:hidden;}
#monitorAll ul li.below a.good{border-top:#76ee01 2px solid;}
#monitorAll ul li.below a.bad{border-top:#f00 2px solid;}

.detailAll{width:118px; height:92px; background:url(../images/detail.png) no-repeat left top; position:absolute; z-index:9999; padding:4px 12px; display:none; top:-116px; left:-42px;}
.detailTitle{height:32px; line-height:32px; color:#fff; font-size:14px;}
.detailContent{ color:#7e7e7e; line-height:19px; padding-top:3px;}
.detailContent span{display:block; width:49%; float:left;}
#monitorAll ul li.below .detailAll{top:auto; bottom:-122px; left:auto; right:-105px; background:url(../images/detail_2.png) no-repeat left top; padding-top:8px;}

.footer{width:100%; padding:12px 0; line-height:20px; color:#2c2c2c; text-align:center; font-size:12px; font-family:Verdana, Geneva, sans-serif; background:#f0f0f0; border-top:#e0e0e0 1px solid;}


/*二级页面样式*/
.subTitle{height:50px; line-height:50px; color:#0168b7; font-size:24px; font-family:"微软雅黑"; background:url(../images/jk_icon_1.png) no-repeat left 7px; padding-left:53px; margin-top:5px;}
.subContent{height:165px; padding:8px 0;font-family:"微软雅黑"; color:#494b5a; font-size:14px;}
.subContent ul{list-style:none; width:300px; padding:5px 0; font-size:14px; float:left;}
.subContent ul li{line-height:31px;}
.subContent ul li span{display:inline-block; width:80px;}
.subContent ul li span.normal{width:auto; background:url(../images/normal.png) no-repeat right 10px; padding-right:16px;}
.detailColumn{width:335px; margin-left:16px; float:left;}
.detailBack{width:325px; height:113px; display:inline-block; background:url(../images/detailBack.png) no-repeat left top; text-align:center; padding:5px;position:relative; text-align:left;}
.detailBack ul{display:block; position:absolute; width:100%; height:32px; line-height:32px; bottom:0; text-align:left;}
.detailBack ul li{display:inline-block; width:32%; padding:0; margin:0; font-size:12px; text-align:center;}
.clumnTitle{font-size:14px; padding: 12px 0 3px 0;}
.clumnTitle span.normal{background:url(../images/normal.png) no-repeat right 5px; padding-right:16px;}
.percent{margin:8px 14px 0 18px;}
.receiveData, .sentData{margin:0 3px; line-height:50px; font-size:16px; text-align:center;}
.receiveData{border-bottom:#dededa 1px solid; color:#33b904; margin-top:5px;}
.sentData{border-top:#fff 1px solid; color:#1862fd;}
.receiveData span, .sentData span{display:inline-block; padding:0 40px 0 17px; font-size:18px; font-weight:bold;}
.receiveData span{background:url(../images/5.png) no-repeat right 11px;}
.sentData span{background:url(../images/6.png) no-repeat right 11px;}
.titleIcon2{background:url(../images/jk_icon_2.png) no-repeat left 7px;}
.titleIcon3{background:url(../images/jk_icon_3.png) no-repeat left 7px;}
.titleIcon4{background:url(../images/jk_icon_4.png) no-repeat left 9px; padding-left:34px;}

.detailTable{padding:6px 0 12px 0;}
.detailTable table{margin-bottom:10px; font-family:"微软雅黑"; color:#4a4c4b;}
.detailTable table thead td{background:#f7fafa; border-bottom:#75d3ce 1px solid; line-height:68px; color:#00857e; font-size:18px; text-align:center; padding:0 5px;}
.detailTable table tbody td{line-height:54px; font-size:14px; text-align:center; border-bottom:#dce0e2 1px solid;}
.detailTable table tbody tr:hover{background:#f8f9fa;}

.all_squares{height:140px; position:relative;  font-family:"微软雅黑"; color:#4a4c4b;}
.square1, .square2, .square3{width:326px; height:136px; float:left; position:relative;}
.square3{z-index:9;}
.square1{background:url(../images/square1.png) no-repeat left top; padding-left:90px; width:236px; line-height:118px; font-size:18px; text-align:center;}
.square2{background:url(../images/square2.png) no-repeat left top; margin-left:30px;}
.square3{background:url(../images/square3.png) no-repeat left top; float:left; margin-left:30px;}
.square2 ul ,.square3 ul{list-style:none; width:212px; position:absolute; left:102px; top:14px; font-size:18px; color:#676767;}
.square2 ul li, .square3 ul li{line-height:46px;}
.square2 ul li, .square3 ul li{text-align:center;}
.square2 ul li.border_bottom, .square3 ul li.border_bottom{background:url(../images/border-bottom.png) repeat-x bottom;}
.square1 ul li span{display:inline-block; width:105px; text-align:right;}
.square2 ul li.green_font, .square3 ul li.green_font{color:#fb8302;}
.square2 ul li.blue_font, .square3 ul li.blue_font{color:#00ada4;}
.tjChart{border:#f0f0f0 2px solid; text-align:center; padding:5px; height:300px; margin-bottom:20px;}
.tabTitle{height:36px;}
.tabTitle ul{list-style:none;}
.tabTitle ul li{position:relative; padding-right:18px; background:url(../images/tabRight.png) no-repeat right top; float:left; height:36px; line-height:38px; cursor:pointer; color:#878787; font-weight:bold; font-size:14px;}
.tabTitle ul li:hover{color:#00b1a8; text-decoration:underline;}
.tabTitle ul li:nth-child(2){left:-3px;}
.tabTitle ul li:nth-child(3){left:-6px;}
.tabTitle ul li:nth-child(4){left:-9px;}
.tabTitle ul li:nth-child(5){left:-12px;}
.tabTitle ul li:nth-child(6){left:-15px;}
.tabTitle ul li:nth-child(7){left:-18px;}
.tabTitle ul li:nth-child(8){left:-21px;}
.tabTitle ul li span{display:block; padding-left:10px; background:url(../images/tabLeft.png) no-repeat left top; height:36px;}
.tabTitle ul li.cur{background:url(../images/hoverRight.png) no-repeat right top; line-height:36px; color:#00857e; z-index:88; top:1px;}
.tabTitle ul li.cur span{background:url(../images/hoverLeft.png) no-repeat left top;}
.tabContent{border:#e0e0e0 1px solid; border-bottom-left-radius:4px; border-bottom-right-radius:4px; background:#fff; padding:20px; height:480px;}

/*监控页面*/
.monitorMain{width:100%; margin:12px 0; font-size:12px; font-family:Verdana, Geneva, sans-serif;}
#roundAll{width:406px; height:406px; float:left; background:url(../images/roundBack.png) no-repeat left top; margin:20px 0 0 5px;}
#center{width:86px; height:86px;position:relative; border-radius:48px; padding:0; margin:0; float:left; overflow:visible;}
#center span{display:block; width:86px; height:86px; background:url(../images/center.png) no-repeat left top; position:absolute; left:160px; top:-245px; text-align:center; line-height:86px; font-size:14px; color:#737373; font-family:"微软雅黑";}

.sortAll{width:470px; float:right; margin:30px 0 0 0;}
.monthAll{height:32px; background:url(../images/monthBack.png) no-repeat left top; color:#9a9a9a; position:relative;}
.year{width:60px; height:18px; overflow:visible; position:relative; float:left; margin:7px 0 0 10px; line-height:18px;}
.year span{display:inline-block; width:42px; height:18px; color:#fff; text-align:center; margin-right:5px; border-radius:3px; background:#9a9a9a; cursor:pointer;}
.year ul{list-style:none; display:inline-block; width:202px; height:110px; border-radius:3px; position:absolute; left:0; top:21px; background:#9a9a9a; color:#fff; padding:5px 5px 0 5px; overflow-y:auto; display:none;}
.year ul li{display:inline-block; width:40px; margin:0 0 5px 5px; text-align:center; height:18px; border:#9a9a9a 1px solid; border-radius:3px; cursor:pointer;}
.year ul li:hover{border:#838383 1px solid; background:#a4a4a4;}
.year ul li.selected{border:#e8e8e8 1px solid; background:#e8e8e8; color:#00857e;}
.monthLine{width:345px; height:26px; background:url(../images/monthLine.png) repeat-x center; float:right; position:relative; top:3px; right:35px;}
.labelM{width:20px; height:32px; line-height:32px; position:absolute; right:2px; top:0;}
.monthLine ul{list-style:none; position:relative; left:-7px;}
.monthLine ul li{display:inline-block; width:12px; height:12px; border-radius:10px; background:#9a9a9a; overflow:hidden; position:absolute; top:7px; cursor:pointer; text-align:center;}
.monthLine ul li:hover{background:#9f9f9f; width:24px; height:24px; border-radius:13px; line-height:24px; top:0px; color:#fff; margin-left:-5px; top:1px;}
.monthLine ul li.selected{background:#00bbb1; width:22px; height:22px; border-radius:13px; line-height:22px; top:0px; color:#fff; margin-left:-4px; top:2px;}
.queue{margin:10px 0;}
.queue ul{list-style:none;}
.queue ul li{height:14px; padding:11px 8px;}
.queue ul li span, .queue ul li h1, .queue ul li h6{display:block; float:left; font-family:Verdana, Geneva, sans-serif; line-height:14px; color:#504f4f;}
.queue ul li span{width:62px;}
.queue ul li h6{width:100px; text-align:right; font-weight:normal;}
.queue ul li h1{width:270px;}
.queue ul li h1 div{height:14px; line-height:14px; overflow:hidden; text-indent:-200px; background:#cfcfcf; float:left;}
.queue ul li:hover div{background:#00bbb1;}
.queue ul li:hover span, .queue ul li:hover h6{color:#00bbb1;}
.point ul li span{width:92px;}
.point ul li h1{width:250px;}

#nodeShow{width:480px; height:490px; float:left; position:relative;}
#nodeShow li{cursor:pointer; position:absolute; color:#fff; font-family:微软雅黑;}
	@-webkit-keyframes 'bounce' {
	from {}
	50% {
		-moz-transform: translateY(-8px);
		-webkit-transform: translateY(-8px);
		-o-transform: translateY(-8px);
		-ms-transform: translateY(-8px);
		transform: translateY(-8px);
	}
	to {
		-moz-transform: translateY(0);
		-webkit-transform: translateY(0);
		-o-transform: translateY(0);
		-ms-transform: translateY(0);
		transform: translateY(0);
	}
}
#nodeShow li h6{font-weight:normal;}
.bigCirle{width:160px; height:100px; border-radius:95px; background:#ff6f5c; position:absolute; color:#fff; text-align:center; padding-top:60px; font-size:18px; line-height:24px;
	-moz-animation: 'bounce' 8s infinite;
	-webkit-animation: 'bounce' 8s infinite;
	-o-animation: 'bounce' 8s infinite;
	-ms-animation: 'bounce' 8s infinite;
	animation: 'bounce' 8s infinite;
}
.bigCirle h6{font-size:18px; font-weight:normal;}
.midCirle{width:90px; height:68px; border-radius:60px; background:#38deba; position:absolute; color:#fff; text-align:center; padding:32px 5px 0 5px; font-size:14px; line-height:20px;
	-moz-animation: 'bounce' 5s infinite;
	-webkit-animation: 'bounce' 5s infinite;
	-o-animation: 'bounce' 5s infinite;
	-ms-animation: 'bounce' 5s infinite;
	animation: 'bounce' 5s infinite;
}
.midCirle h6{font-size:12px; font-family:微软雅黑;}
.smallCirle{width:60px; height:50px; border-radius:34px; background:#6fc5fe; position:absolute; color:#fff; text-align:center; padding:22px 6px 0 6px; font-size:12px; line-height:13px;
	-moz-animation: 'bounce' 3s infinite;
	-webkit-animation: 'bounce' 3s infinite;
	-o-animation: 'bounce' 3s infinite;
	-ms-animation: 'bounce' 3s infinite;
	animation: 'bounce' 3s infinite;
}
.smallCirle span{display:block; width:100%;height:13px; overflow:hidden; text-align:center;}
.smallCirle h6{font-size:12px; font-family:微软雅黑;}
#nodeShow li:hover{z-index:999;}
.bigCirle:hover{border:#ff6f5c 10px solid; background:#fff; width:190px; height:125px; padding-top:65px; border-radius:105px; margin:-15px 0 0 -15px;}
.bigCirle:hover span, .bigCirle:hover h6{color:#ff6f5c;}
.midCirle:hover{border:#38deba 10px solid; background:#fff; width:130px; height:95px; padding-top:45px; border-radius:100px; margin:-15px 0 0 -15px;}
.midCirle:hover span, .midCirle:hover h6{color:#38deba;}
.smallCirle:hover{border:#6fc5fe 8px solid; background:#fff; width:100px; height:65px; padding-top:45px; border-radius:100px; margin:-15px 0 0 -15px;}
.smallCirle:hover span, .smallCirle:hover h6{color:#6fc5fe; display:block; width:100%;}
#domain, .recentTitle, #recent10{width:80%; margin:0 auto; text-align:center;}
.recentTitle{margin-top:30px; font-weight:bold; font-size:16px; color:#4b4b4b; font-family:"微软雅黑";}
#areaSort{width:490px; float:left; height:300px; margin:30px 0 0 -15px;}

.subTitle{position:relative; color:#00857e;}
.subTitle span{display:block; width:60px; text-align:right; position:absolute; right:0; top:0; cursor:pointer; font-size:14px; text-decoration:underline; color:#6e6e6e;}
.subTitle span:hover{text-decoration:none; color:#fb8302;}
.reportAll{position:relative; height:380px;}
.reportTable{height:315px; position:absolute; top:50px; left:0; background:#fff; padding:0 0 1px 1px; display:none;}
.reportTable ul li{float:left; width:139px; height:26px; line-height:26px; background:#fff; border:#dcdcdc 1px solid; margin:1px 1px 0 0; padding:0 12px; font-size:12px; color:#fb8302;}
