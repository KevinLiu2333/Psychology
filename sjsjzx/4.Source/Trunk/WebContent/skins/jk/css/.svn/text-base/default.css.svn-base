/*20150704 jiaojun*/
.detailTable table{margin-bottom:10px;}
.detailTable table thead td{background:#f5f9fd; border-bottom:#b2d2f2 1px solid; line-height:68px; color:#276ab0; font-size:18px; text-align:center; padding:0 5px;}
.detailTable table tbody td{line-height:54px; font-size:14px; text-align:center; border-bottom:#dce0e2 1px solid;}
.detailTable table tbody tr:hover{background:#f8f9fa;}
.detailTable table tbody td span{color:#3384d5; cursor:pointer;}
.detailTable table tbody td span:hover{text-decoration:underline;}
.detailTable table tbody tr.subTable table td{border-bottom:none; line-height:38px; border-bottom:#dce0e2 1px dotted; background:#f2f8fe;}
.detailTable table tbody tr.subTable table tr:last-child td{border-bottom:none;}
.detailTable table tbody tr.subTable table{margin:0; border-bottom:#24bbfa 3px solid;}
.detailTable table tbody tr.subTable{display:none;}
