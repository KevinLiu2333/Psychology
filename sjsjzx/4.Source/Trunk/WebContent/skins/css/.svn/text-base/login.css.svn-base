@charset "utf-8";
/* CSS Document */

body{
	margin:0px;
	padding:0px;
	background:#d3e4f2  url(../images/login/login_bg.jpg) repeat-x  center;
	height:600px;
	color:#fff;
	font-family:Arial, Helvetica, sans-serif;
	font-size:14px;
	}
#login_bg{
	background:url(../images/login/login_index.jpg) no-repeat center;
	height:600px;
	}
.logo_input_box_user{
	background:#FFF url(../images/login/logo_man.jpg) no-repeat left;
	height:30px;
	width:220px;
	border:1px #0566a1 solid;
	padding:0px 0px 0px 5px;
	line-height:30px;
	}
.login_in{
	background:url(../images/login/login_in.jpg) no-repeat;
	height:41px;
	width:100px;
	border:0px;
	cursor:pointer;
	}
.login_out{
	background:url(../images/login/login_out.jpg) no-repeat;
	height:41px;
	width:100px;
	border:0px;
	margin-left:25px;
	cursor:pointer;
	}