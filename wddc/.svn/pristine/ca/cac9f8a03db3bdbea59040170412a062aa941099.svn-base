package com.wonders.wddc.suite.csrq.entity;

import org.nutz.dao.entity.annotation.Column;
import org.nutz.dao.entity.annotation.Name;
import org.nutz.dao.entity.annotation.Table;
import org.nutz.lang.Strings;

/**
* 
*/
@Table("SUITE_QUERY_CONFIG")
public class QueryConfigBo {

	
	@Name
	@Column("SAVE_ID")
	private String saveId;
	
	/**
	 * 
	 */
	@Column("THEME_ID")
	private Integer themeId;
	
	/**
	 *
	 */
	@Column("USER_ID")
	private String userId;
	
	/**
	 *
	 */
	@Column("USER_NAME")
	private String userName;

	/**
	 * 
	 */
	@Column("FORM_ID")
	private String formId;
	
	/**
	 * 
	 */
	@Column("NAME")
	private String name;
	
	/**
	 * 
	 */
	@Column("CON_COLS")
	private String conCols;
	
	/**
	 * 
	 */
	@Column("RS_COLS")
	private String rsCols;
	
	/**
	 * 
	 */
	@Column("SORT_COLS")
	private String sortCols;
	
	/**
	 * 
	 */
	@Column("SAVE_DATE")
	private java.util.Date saveDate;
	

	
	/**
	 * 
	 */
	@Column("SAVE_DESC")
	private String saveDesc;
	
	/**
	 * 
	 */
	@Column("RS_PREVIEW")
	private String rsPreview;
	

	
	/**
	 * 
	 */
	@Column("RENDERER")
	private String renderer;

	/**
	 * 
	 */
	@Column("WHERE_COLS")
	private String whereCols;

	/**
	 * 
	 */
	@Column("WHERE_JOINS")
	private String whereJoins;

	/**
	 * 
	 */
	@Column("WHERE_VALUES")
	private String whereValues;

	/**
	 * 
	 */
	@Column("WHERE_APPEND")
	private String whereAppend;

	/**
	 * 
	 */
	@Column("VIEW_TYPE")
	private String viewType;
	/**
	 * 查询配置标签
	 * @return
	 */
	@Column("TAG_LIST")
	private String tagList;
	
	public String[] getTags() {
	    if(!Strings.isBlank(tagList)){
	            return tagList.split(",");
	    }
	    return null;
	}
	public String getTagList() {
		return tagList;
	}

	public void setTagList(String tagList) {
		this.tagList = tagList;
	}
	
	public String getViewType() {
		return viewType;
	}
	public void setViewType(String viewType) {
		this.viewType = viewType;
	}
	public String getWhereCols() {
		return whereCols;
	}
	public void setWhereCols(String whereCols) {
		this.whereCols = whereCols;
	}
	public String getWhereJoins() {
		return whereJoins;
	}
	public void setWhereJoins(String whereJoins) {
		this.whereJoins = whereJoins;
	}
	public String getWhereValues() {
		return whereValues;
	}
	public void setWhereValues(String whereValues) {
		this.whereValues = whereValues;
	}
	public String getWhereAppend() {
		return whereAppend;
	}
	public void setWhereAppend(String whereAppend) {
		this.whereAppend = whereAppend;
	}
	public String getSaveId() {
		return saveId;
	}
	public void setSaveId(String saveId) {
		this.saveId = saveId;
	}
	
	public Integer getThemeId() {
		return themeId;
	}
	public void setThemeId(Integer themeId) {
		this.themeId = themeId;
	}
	
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public String getConCols() {
		return conCols;
	}
	public void setConCols(String conCols) {
		this.conCols = conCols;
	}
	
	public String getRsCols() {
		return rsCols;
	}
	public void setRsCols(String rsCols) {
		this.rsCols = rsCols;
	}
	
	public String getSortCols() {
		return sortCols;
	}
	public void setSortCols(String sortCols) {
		this.sortCols = sortCols;
	}
	
	public java.util.Date getSaveDate() {
		return saveDate;
	}
	public void setSaveDate(java.util.Date saveDate) {
		this.saveDate = saveDate;
	}
	
	public String getSaveDesc() {
		return saveDesc;
	}
	public void setSaveDesc(String saveDesc) {
		this.saveDesc = saveDesc;
	}
	
	
	public String getRenderer() {
		return renderer;
	}
	public void setRenderer(String renderer) {
		this.renderer = renderer;
	}
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getFormId() {
		return formId;
	}
	public void setFormId(String formId) {
		this.formId = formId;
	}
	public String getRsPreview() {
		return rsPreview;
	}
	public void setRsPreview(String rsPreview) {
		this.rsPreview = rsPreview;
	}
	
	
}