		<script type="text/javascript">
            function loaddata(){
                $.ajax({
                    type:'get',
                    url:"json/czrz.json",
                    dataType:"text",
                    success:function(data){
                        data = eval('('+data+')');
                        var sjcx = data.sjcx;
                        var fwdy = data.fwdy;
                        $("#zyml").html(data.zyml[0].value)
                        $("#mlfb").html(data.zyml[1].value)
                        $("#mlll").html(data.zyml[2].value)
                        var sjcxstr = "";
                        var fwdystr = "";
                        for(var i=0;i<sjcx.length;i++){
                            sjcxstr = sjcxstr+'<div class="infobox infobox-red" ><div class="infobox-icon"><i class="ace-icon  glyphicon glyphicon-folder-open"></i></div><div class="infobox-data"><span class="infobox-data-number">'+sjcx[i].value+'次</span><div class="infobox-content">'+sjcx[i].name+'</div></div></div>'
                        }
                        for(var i=0;i<fwdy.length;i++){
                            fwdystr = fwdystr+'<div class="infobox infobox-green" ><div class="infobox-icon"><i class="ace-icon  glyphicon glyphicon-resize-full"></i></div><div class="infobox-data"><span class="infobox-data-number">'+fwdy[i].value+'次</span><div class="infobox-content">'+fwdy[i].name+'</div></div></div>'
                        }
                        console.log(data.zyml[0].value)
                        $('#sjcx').html(sjcxstr);
                        $('#fwdy').html(fwdystr);
                    }
                });
                //setTimeout("loaddata()",1000*30);  
            }
            loaddata();
            $.ajax({
                type:'get',
                url:"json/czrz_jt.json",
                dataType:"text",
                success:function(data){
                    data = eval('('+data+')');
                    require.config({
		      paths: {
                  echarts: 'assets/echart'
                }
            });
            require([
                        'echarts',
                        'echarts/chart/pie',
                        'echarts/chart/bar'
                    ],function (ec){
                var myChart= ec.init(document.getElementById("d1"));
                var option = {
                    title : {
                        text: '各部门操作统计',
                    },
                     legend: {
                         data:['数据查询', '服务调用','资源目录'],
                         x:'right',
                    },
                    tooltip : {
                        trigger: 'axis'
                    },
                    calculable : true,
                    xAxis : [
                        {
                            type : 'value',
                            boundaryGap : [0, 0.01]
                        }
                    ],
                    yAxis : [
                        {
                            type : 'category',
                            data : data.axis
                        }
                    ],
                    series : [
                        {
                            name:'数据查询',
                            type:'bar',
                            stack: '总量',
                            data:data.sjcx,
                            itemStyle : { normal: {color:'#D15B47'}}
                        },
                        {
                            name:'服务调用',
                            type:'bar',
                            stack: '总量',
                            data:data.fwdy,
                            itemStyle : { normal: {color:'#84B357',}}
                        },
                        {
                            name:'资源目录',
                            type:'bar',
                            stack: '总量',
                            data:data.zyml,
                            itemStyle : { normal: {color:'#5AB1EF'}}
                        }
                    ]
                };
                myChart.setOption(option);
            });
                }
            });
            
            $("#header").load("include/header.html");
            $("#footer").load("include/footer.html");
            $("#sidebar").load("include/sidebar.html",function(response,status,xhr){
                $('#jksy').removeAttr('class');
                $('#czjk').attr('class','active');
                $('#czrz').attr('class','active');
            });

		</script>
