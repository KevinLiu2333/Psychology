        <script type="text/javascript">
		// include
            $("#header").load("include/header.html");
            $("#sidebar").load("include/sidebar.html",function () {
                $("#jksy").attr("class","");
                $("#jhjk").attr("class","active");
                $("#jhzltj").attr("class","active");
            });
            $("#footer").load("include/footer.html");
			
		     $.ajax({
				url:"json/jhzlb.json",
				type:"GET",
				dataType: "text",
				success:function(data){
					var data = eval('('+data+')');
				    var jhzlb = data.jhzlb;
					var html = '';
					for(var i=0;i<jhzlb.length;i++){
					    var index = i+1;
					    html=html+ '<tr class="center"> <td>'+index+'</td><td><a href="jd.html?name=1">'+jhzlb[i].deptment+'</a></td><td>'+jhzlb[i].jhcs+'</td><td>'+jhzlb[i].jszl+'</td><td>'+jhzlb[i].wtsjl+'</td><td>'+jhzlb[i].wtsjzb+'</td><td>'+jhzlb[i].lasttime+'</td></tr>';
					}
					$('#tdata').html(html);
					
                }
			 });
            if('ontouchstart' in document.documentElement) document.write("<script src='assets/js/jquery.mobile.custom.min.js'>"+"<"+"/script>");
        </script>
