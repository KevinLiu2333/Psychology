<!DOCTYPE html>
<html>
<head>
    <title>单行录入框</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <link rel="stylesheet" href="bootstrap/css/bootstrap.css">
    <script type="text/javascript" src="jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="../dialogs/internal.js"></script>
</head>
<body>

<div class="panel-group" id="accordion">

  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
          页面组件
        </a>
      </h4>
    </div>
    <div id="collapseOne" class="panel-collapse collapse in">
      <div class="panel-body">
		<button type="button" onclick="dreamForm.exec('dream-text');" class="btn btn-default">单行录入框</button>
		<button type="button" onclick="dreamForm.exec('dream-password');" class="btn btn-default">密码域</button>
		<button type="button" onclick="dreamForm.exec('dream-hidden');" class="btn btn-default">隐藏域</button>
		<button type="button" onclick="dreamForm.exec('dream-date');" class="btn btn-default">日期域</button>
		<button type="button" onclick="dreamForm.exec('dream-select');" class="btn btn-default">下拉列表框</button>
		<button type="button" onclick="dreamForm.exec('dream-textarea');" class="btn btn-default">多行录入框</button>
		<button type="button" onclick="dreamForm.exec('dream-radio');" class="btn btn-default">单选框</button>
		<button type="button" onclick="dreamForm.exec('dream-checkbox');" class="btn btn-default">复选框</button>
      </div>
    </div>
  </div>
  
  <div class="panel panel-success">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion"  href="#collapseTwo">
          复合组件
        </a>
      </h4>
    </div>
    <div id="collapseTwo" class="panel-collapse collapse">
      <div class="panel-body">
		<button type="button" onclick="dreamForm.exec('dream-subtable');" class="btn btn-default">子表编辑</button>
      </div>
    </div>
  </div>
  
  <div class="panel panel-info">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion"  href="#collapseThree">
          字典组件
        </a>
      </h4>
    </div>
    <div id="collapseThree" class="panel-collapse collapse">
      <div class="panel-body">
		<button type="button" onclick="#" class="btn btn-default">性别</button>
		<button type="button" onclick="#" class="btn btn-default">国籍</button>
		<button type="button" onclick="#" class="btn btn-default">行政区划</button>
		<button type="button" onclick="#" class="btn btn-default">学历</button>
      </div>
    </div>
  </div>
  
  <div class="panel panel-warning">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion"  href="#collapseFour">
          数据库组件
        </a>
      </h4>
    </div>
    <div id="collapseFour" class="panel-collapse collapse">
      <div class="panel-body">
		<button type="button" onclick="#" class="btn btn-default">表</button>
		<button type="button" onclick="#" class="btn btn-default">字段</button>
      </div>
    </div>
  </div>
  
</div>

<script type="text/javascript">
var dreamForm = {
    exec : function (method) {
        editor.execCommand(method);
        dialog.close(true);
    }
};
</script>
</body>
</html>