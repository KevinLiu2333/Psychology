#item li{list-style:none;display:block;border:0px solid #ccc;}
#item .in{display:block;}

.subj{height: 410px;}
.subj ul li{
    float: left;
    width: 90px;
    text-align: center;
    margin: 30px 60px 0 10px;
    font-family: "微软雅黑","宋体";
    color:black;
    list-style:none;
}
.subj ul li a:hover span{
    color: #ED8E22;
}
.subj ul li img{border-bottom: solid 1px #999;}

.subj ul li a span{
	color: #404040;
	font-weight: bold;
}

 .nav  > ul > li.navLogin{
	width:200px;
	border:1px solid #000000;
	float:right;
}

 .nav > ul > li.navLogin a{
    width: 50px;
    height: 100%;
    color: red;
 }

