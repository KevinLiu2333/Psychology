/*共有*/
.second_left,.third_left,.fourth_left,.fifth_left{float:left;width:672px;height:279px;}
.first,.second,.third,.fourth,.fifth{margin-top:10px; overflow:hidden;}
.title{width:656px;height:44px;padding-top:12px; overflow:hidden;}
.title span{line-height:40px;font-family:"微软雅黑";font-size:18px;color:#0f51b2;float:left; display:block;}
.title span img{margin:0 10px 0 20px;}
.title a{color:#ffffff;font-family:"微软雅黑";font-size:14px;background:#5187d7;text-align:center;width:60px;height:20px;padding:2px 4px;-moz-border-radius:4px;-webkit-border-radius:4px;border-radius:4px; float:right;margin:10px 10px 0 0;display:block;}
.title a:hover{background:#2b69c6;}
.second_left dl,.fourth_left dl{width:634px;margin:0 auto;position:relative;}
.second_left dl dt,.fourth_left dl dt{float:left;cursor:pointer;margin:0 5px;padding:10px 16px;font-family:"微软雅黑";font-size:16px;color:#7d7d7d;}
.second_left dl .click_info,.fourth_left dl .click_info{color:#0f51b2;font-weight:bold; border-bottom:#0f51b2 2px solid}
.second_left dl dd,.fourth_left dl dd{position:absolute;left:0;top:54px;}
.second_left dl dd p,.fourth_left dl dd p{line-height:36px;font-family:"微软雅黑";font-size:16px;color:#535353; overflow:hidden;cursor:pointer;}
.second_left dl dd p:hover,.fourth_left dl dd p:hover{color:#C02B2D;}
.second_left dl dd p img,.fourth_left dl dd p img{margin-right:6px;}
.second_left dl dd p span,.fourth_left dl dd p span{display:block; float:left;}
.textCon{width:380px;}
.liulan{width:160px;color:#a7a7a7;}
.day{color:#737373;}
/*second_right*/
.second_right{width:328px;height:279px;border-left:#a0bada 1px dashed;float:right;margin-bottom:8px;}
.fourth_right{width:328px;height:279px; background:url(../images/index/right_bg_ditu.png) no-repeat;border-left:#a0bada 1px dashed; float:right;margin-bottom:8px; position:relative;}
.fourth_right a img{position:absolute;right:40px;bottom:40px;}
.tubao_info{width:164px;float:right;margin:60px 10px 0 0;}
.tubao_info h5{font-family:"微软雅黑";font-size:16px;color:#0f51b2;line-height:36px;}
.tubao_info h5 span{font-family:arial,"微软雅黑";font-size:24px;color:#dd361f;font-weight:bold;}
.tubao_info h3{font-family:"微软雅黑";font-size:18px;color:#0f51b2;line-height:60px;font-weight:bold; padding-left:30px;}
/*third_bnt*/
.third_bnt{width:170px;height:40px;margin:0 auto;}
.third_bnt ul{border:#0f51b2 1px solid; overflow:hidden;margin-top:10px;border-radius:5px;-webkit-border-radius:5px;-moz-border-radius: 5px;}
.third_bnt ul li{float:left;cursor:pointer;height:30px;font-family:"微软雅黑";font-size:16px;color:#5380c2;padding:0 26px;line-height:30px;}
.third_bnt ul .click_bnt{background:#0f51b2;color:#ffffff;font-weight:bold;}
/*third_info*/
.third_con{width:530px;height:150px;margin:4px auto; overflow:hidden;}
.third_info{width:1600px;}
.third_info li{width:249px;height:150px;background:url(../images/index/info_con.png) no-repeat;float:left;margin-right:20px;}
.third_info li h3{font-family:"微软雅黑";font-size:16px;color:#357ed3;font-weight:bold;line-height:36px; text-align:center;}
.third_info li p{width:208px;margin:12px auto;color:#3514d4d;line-height:26px;font-family:"微软雅黑";font-size:16px;}
.third_info li h6{font-family:"微软雅黑";font-size:16px;color:#d7000f;font-weight:bold;line-height:20px; text-align:center;}
/*ctrl*/
.third_left{position:relative;}
.ctrl_left{width:18px;height:33px;float:left;cursor:pointer;position:absolute;top:170px;left:20px;bottom:0;background:url(../images/index/iconfont-ctrlleft.png) no-repeat;}
.ctrl_right{width:18px;height:33px;float:right; cursor:pointer;position:absolute;top:170px;right:20px;bottom:0;background:url(../images/index/iconfont-ctrlRight.png) no-repeat top right;}
/*fifth*/
.fifth{width:1008px;height:279px;}
.fifth ul{margin-top:26px; overflow:hidden;}
.fifth ul li{width:286px;height:226px; background:url(../images/index/fi_con_bg.png) no-repeat;margin:0 25px; float:left;}
.fifth ul li h3{width:286px;font-family:"微软雅黑";font-size:16px;color:#ffffff;font-weight:bold;line-height:42px; text-align:center;margin-bottom:20px;}
.fifth_con{width:242;margin:0 auto; overflow:hidden;}
.fifth ul li p{font-family:"微软雅黑";font-size:16px;color:#171717;line-height:36px;display:block;float:left;margin-left:30px;}
.fifth ul li span{display:block;float:right;margin-right:30px;font-family:"微软雅黑";font-size:16px;color:#171717;line-height:36px;}
/*footer*/
.footer{width:100%;height:48px;background:#0f51b2;}
.footer span{display:block;width:1002px;margin:0 auto;}
.footer span h5{font-family:"微软雅黑";font-size:16px;color:#ffffff;float:left;margin-left:40px;line-height:48px;}
.footer span p{font-family:"微软雅黑";font-size:16px;color:#ffffff;float:right;margin-right:40px;line-height:48px;}