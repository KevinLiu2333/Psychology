@charset "utf-8";
/*重置*/
*{margin:0;padding:0;}
a{text-decoration:none;}
ul,li,dl,dt,dd{list-style:none;}
img{border:none;}
h1,h2,h3,h4,h5,h6{font-weight:normal;line-height: normal;}
html,body{background:#ffffff;}
/*header*/
.header{width:100%;height:40px; background-color:#ffffff;}
.header_info{width:1048px; margin:0 auto; overflow:hidden; background:url(../images/index/header_bg_02.png) bottom right no-repeat;}
.header_info img{float:left;margin-top:17px;}
.header_info a{float:right;padding:0 20px;line-height:82px;color:#0f51b2; font-family:"微软雅黑";}
/*banner*/
.banner{width:100%;height:250px;background-color:#43a8fd;margin-top:1px;}
.banner_box{width:1048px;margin:0 auto;overflow:hidden; position:relative;}
.box_photo{width:9999px;float:left;overflow:hidden;}
.box_photo li{float:left;}
.banner_btn {position:absolute;width:120px;bottom:14px;left:451px;z-index: 33;overflow: hidden;}
.banner_btn span {display: block;width:12px;height:12px;background:url(../sjdc/skins/images/index/banner_y_01.png) no-repeat;margin: 0 12px;cursor: pointer; float:left;}
.banner_btn .current {width:14px;height:14px;background:url(../sjdc/skins/images/index/banner_y_02.png) no-repeat;}
/*走马灯*/
.zmd_text{height:50px;border-top:#CDEBFF 1px solid;border-bottom:#CDEBFF 1px solid; margin-top:10px;position:relative;}
.textInfo{width:1002px;margin:0 auto;overflow:hidden;position:relative;height:60px;}
.textInfo li{width:1200px;position:absolute;}
.textInfo li a{margin-right:120px;float:left;line-height:50px;color:#124ba0; font-family:"微软雅黑";font-size:16px;}
/*content*/
.content{width:1300px;margin:12px auto;}
.first,.second,.third,.fourth{border-bottom:#a0bada 1px dashed;}
.first_left{float:left;}
.first_left ul li{width:336px;height:186px;float:left;}
.first_left ul li img{margin:50px 20px;float:left;}
.first_left ul li span{display:block;width:150px;height:178px;float:left;}
.span01{background-image:url(../images/index/bg_shu001.png); background-repeat:no-repeat; background-position:4px 16px;}
.span02{background-image:url(../images/index/bg_shu001.png);background-repeat:no-repeat;background-position:4px 16px;}
.span03{background-image:url(../images/index/bg_shu001.png);background-repeat:no-repeat;background-position:4px 16px;}
.first_left ul li span h2{margin-top:65px;font-family:"微软雅黑";font-size:28px;color:#dd361f; text-align:center;}
.first_left ul li span h3{font-family:"微软雅黑";font-size:20px;color:#7e81e2; text-align:center;}
.first_left ul li span p{display:block;width:100px;height:2px;background:#898be4;margin:0 auto;}
.first_right{float:right;width:328px;height:186px;}

.subj{height: 410px;}
.subj ul li{
    float: left;
    width: 90px;
    text-align: center;
    margin: 40px 40px 0 40px;
    font-family: "微软雅黑","宋体";
}
.subj ul li a:hover{color: #3b99d8}
.subj ul li img{border-bottom: solid 1px #999;}
.subj .subj_header{
    height: 40px;
    width: 100%;
    border-bottom: solid 1px #999;
    background: url("../../img/gz/header.png") no-repeat;
    font-family:"微软雅黑";font-size:18px;color:#0f51b2;float:left; display:block;
}
